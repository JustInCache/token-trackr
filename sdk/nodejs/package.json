{
  "name": "token-trackr-sdk",
  "version": "1.0.0",
  "description": "Token Trackr SDK for tracking LLM token consumption across AWS Bedrock, Azure OpenAI, and Google Gemini",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "token-trackr",
    "llm",
    "bedrock",
    "azure-openai",
    "gemini",
    "token-tracking",
    "cost-tracking"
  ],
  "author": "Token Trackr Team",
  "license": "MIT",
  "dependencies": {
    "undici": "^6.2.0"
  },
  "devDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.460.0",
    "@azure/openai": "^1.0.0-beta.10",
    "@google/generative-ai": "^0.2.0",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-bedrock-runtime": ">=3.0.0",
    "@azure/openai": ">=1.0.0-beta.0",
    "@google/generative-ai": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-bedrock-runtime": {
      "optional": true
    },
    "@azure/openai": {
      "optional": true
    },
    "@google/generative-ai": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

